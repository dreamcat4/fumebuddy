{
  "title": "Fumebuddy Settings",
  "uri": "/fumebuddy_setting",
  "menu": true,
  "element": [
    {
      "name": "style",
      "type": "ACStyle",
      "value": "label+input,label+select{position:sticky;left:120px;width:230px!important;box-sizing:border-box;}"
    },
    {
      "name": "caption",
      "type": "ACText",
      "value": "Enter URL's that can be called from this device",
      "style": "font-family:serif;color:#4682b4;"
    },
    {
      "name": "urlon",
      "type": "ACInput",
      "value": "",
      "placeholder": "on URL",
      "label": "Power-On URL"
    },
		{
      "name": "urloff",
      "type": "ACInput",
      "value": "",
      "placeholder": "off URL",
      "label": "Power-Off URL"
    },
    {
      "name": "urltoggle",
      "type": "ACInput",
      "value": "",
      "placeholder": "toggle URL3",
      "label": "Power Toggle URL"
    },
    {
      "name": "delay",
      "type": "ACInput",
      "value": "3",
      "placeholder": "delay in seconds",
      "label": "Power-off delay"
    },
    {
      "name": "touchoverride",
      "type": "ACCheckbox",
      "value": "true",
      "placeholder": "touchoverride",
      "label": "Touch Override - ignore all onhook/offhook events when manually activated"
    },
    {
      "name": "beeper",
      "type": "ACCheckbox",
      "value": "true",
      "placeholder": "Beeper",
      "label": "Beeper"
    },
    {
      "name": "save",
      "type": "ACSubmit",
      "value": "Save",
      "uri": "/fumebuddy_save"
    },
    {
      "name": "discard",
      "type": "ACSubmit",
      "value": "Discard",
      "uri": "/"
    }
  ]
}